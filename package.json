{
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "name": "reactjs-saga-starter",
  "version": "1.0.0",
  "description": "A starter kit for all your React.js web applications.",
  "scripts": {
    "start": "better-npm-run dev-server",
    "prod": "better-npm-run prod-server",
    "lint": "better-npm-run lint"
  },
  "betterScripts": {
    "dev-server": {
      "command": "json-server -w db.json & webpack-dev-server --config webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod-server": {
      "command": "json-server -w db.json & webpack -p --config webpack.config.prod.js && node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "node_modules/eslint/bin/eslint.js . --ext .js --ext .jsx --cache"
    }
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.1",
    "cross-env": "^1.0.8",
    "dotenv": "^2.0.0",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.4",
    "express-history-api-fallback": "^2.0.0",
    "object-assign": "^4.1.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.1",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "3.0.0-alpha.13",
    "redux-devtools": "^3.2.0",
    "redux-logger": "^2.6.1",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "json-server": "^0.8.16",
    "react": "^15.0.1",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0"
  }
}
